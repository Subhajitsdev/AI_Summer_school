        "        elif attack_type == \"signature\":\n",
        "          damage = 5\n",
        "        elif attack_type == \"finisher\":\n",
        "          damage = 10\n",
        "\n",
        "        return damage\n",
        "\n",
        "dog = Dog(name=\"spiky\", level=5, life=25)\n",
        "\n",
